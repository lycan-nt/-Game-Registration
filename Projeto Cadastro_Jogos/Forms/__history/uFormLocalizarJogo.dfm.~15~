  object FDQueryLocalizaGenero: TFDQuery
    Connection = DmDados.FDConnection1
    SQL.Strings = (
      
        'select g.nome,'#10'      g.id_genero,'#10'      j.fk_id_genero,'#10'      j.' +
        'fk_id_genero1,'#10'      j.fk_id_genero2,'#10'      j.nome,'#10'      j.id_j' +
        'ogo'#10'from jogo j inner join genero g on j.fk_id_genero = g.id_gen' +
        'ero; ')
