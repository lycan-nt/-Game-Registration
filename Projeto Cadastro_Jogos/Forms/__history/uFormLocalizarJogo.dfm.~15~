  inherited FDQueryLocalizar: TFDQuery
    SQL.Strings = (
      
        'select g.nome,'#10'      g.id_genero,'#10'      j.fk_id_genero,'#10'      j.' +
        'fk_id_genero1,'#10'      j.fk_id_genero2,'#10'      j.jogo_nome,'#10'      j' +
        '.id_jogo,'#10'      j.data_lancamento'#10'from jogo j inner join genero ' +
        'g on j.fk_id_genero = g.id_genero;     ')
